[alias]
    co=checkout
    ci=commit -a
    st=status -s
    di=diff
    up=pull --rebase
    patch=--no-pager diff --no-color

    # logをtree表示
    log-graph = log --graph --date=short --pretty=format:'%h %cd %cn %s'
    log-all = log --graph --all --pretty='%x09%h %cn%x09%s %d'
[color]
    ui = auto
[core]
    pager = diff-so-fancy | less --tabs=4 -RFX
[pull]
	rebase = false
[grep]
	lineNumber = true
